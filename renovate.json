{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly"
  ],
  "automerge": true,
  "baseBranches": [
    "master"
  ],
  "major": {
    "automerge": false
  },
  "node": {
    "supportPolicy": ["current"]
  },
  "schedule": [
        "after 4pm every Friday",
        "before 7am every Saturday",
        "before 7am every Sunday"
  ],
  "travis": {
    "enabled": true
  }
}
